   

              <!-- Bootstrap Table with Header - Dark -->
              <div class="card">
                <h5 class="card-header"></h5>
                <div class="table-responsive text-nowrap">
                  <table class="table">
                    <thead class="table-dark">
                      <tr>
                        <th>Image</th>
                        <th>Name</th>
                        <th>Category</th>
                        <th>Discription</th>
                        <th>Price</th>
                        <th>Actions</th>
                        <th>Actions</th>
                      </tr>
                    </thead>
                    {{#each product}}
                    <tbody class="table-border-bottom-0">
                      
                       <tr>
                   
                    <td> <img  style="height: 75px; width:75px;" src="/productimage/{{this._id}}.jpg" >  </td> 
                    <td>{{this.name}}</td>
                    <td>{{this.category}}</td>                   
                    <td>{{this.discription}}</td>
                      <td>{{this.price}}</td>
                      
                      
                    <br>
                    <td><a  href="/admin/edit-users/{{this._id}}" class="btn btn-primary">Edit User</a></td>
                    <td><a href="/admin/delete-users/{{this._id}}" 
                            onclick="return confirm('Are you want to delete{{this.Name}}?')"
                            class="btn btn-danger">Delete</a></td>


                </tr>
                {{/each}}

                    </tbody>
                  </table>
                </div>
              </div>
              <!--/ Bootstrap Table with Header Dark -->

