
         
            

              <!-- Bootstrap Table with Header - Dark -->
              <div class="card">
                <h5 class="card-header"></h5>
                <div class="table-responsive text-nowrap">
                  <table class="table">
                    <thead class="table-dark">
                      <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Phone</th>
                        <th>Status</th>
                        <th>Actions</th>
                      </tr>
                    </thead>
                    {{#each user}}
                    <tbody class="table-border-bottom-0">
                      
                       <tr>
                   
                    <td>{{name}}</td>
                    <td>{{email}}</td>
                    <td>{{phone}}</td>
                    <br>
                    <td><a href="/admin/edit-users/{{this._id}}" class="btn btn-primary">Edit User</a></td>
                    <td><a href="/admin/delete-users/{{this._id}}" 
                            onclick="return confirm('Are you want to delete{{this.Name}}?')"
                            class="btn btn-danger">Delete</a></td>


                </tr>
                {{/each}}

                    </tbody>
                  </table>
                </div>
              </div>
              <!--/ Bootstrap Table with Header Dark -->

